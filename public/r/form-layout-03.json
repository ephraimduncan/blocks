      "content": "import { Button } from \"@/components/ui/button\";\r\nimport { Checkbox } from \"@/components/ui/checkbox\";\r\nimport { Input } from \"@/components/ui/input\";\r\nimport { Label } from \"@/components/ui/label\";\r\nimport {\r\n  Select,\r\n  SelectContent,\r\n  SelectItem,\r\n  SelectTrigger,\r\n  SelectValue,\r\n} from \"@/components/ui/select\";\r\nimport { Separator } from \"@/components/ui/separator\";\r\nimport { Textarea } from \"@/components/ui/textarea\";\r\n\r\nexport default function Example() {\r\n  return (\r\n    <div className=\"flex items-center justify-center p-10\">\r\n      <form>\r\n        <div className=\"grid grid-cols-1 gap-10 md:grid-cols-3\">\r\n          <div>\r\n            <h2 className=\"font-semibold text-foreground dark:text-foreground\">\r\n              Personal information\r\n            </h2>\r\n            <p className=\"mt-1 text-sm leading-6 text-muted-foreground dark:text-muted-foreground\">\r\n              Lorem ipsum dolor sit amet, consetetur sadipscing elitr.\r\n            </p>\r\n          </div>\r\n          <div className=\"sm:max-w-3xl md:col-span-2\">\r\n            <div className=\"grid grid-cols-1 gap-4 sm:grid-cols-6\">\r\n              <div className=\"col-span-full sm:col-span-3\">\r\n                <Label\r\n                  htmlFor=\"first-name\"\r\n                  className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                >\r\n                  First name\r\n                </Label>\r\n                <Input\r\n                  type=\"text\"\r\n                  id=\"first-name\"\r\n                  name=\"first-name\"\r\n                  autoComplete=\"given-name\"\r\n                  placeholder=\"Emma\"\r\n                  className=\"mt-2\"\r\n                />\r\n              </div>\r\n              <div className=\"col-span-full sm:col-span-3\">\r\n                <Label\r\n                  htmlFor=\"last-name\"\r\n                  className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                >\r\n                  Last name\r\n                </Label>\r\n                <Input\r\n                  type=\"text\"\r\n                  id=\"last-name\"\r\n                  name=\"last-name\"\r\n                  autoComplete=\"family-name\"\r\n                  placeholder=\"Crown\"\r\n                  className=\"mt-2\"\r\n                />\r\n              </div>\r\n              <div className=\"col-span-full\">\r\n                <Label\r\n                  htmlFor=\"email\"\r\n                  className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                >\r\n                  Email\r\n                </Label>\r\n                <Input\r\n                  type=\"email\"\r\n                  id=\"email\"\r\n                  name=\"email\"\r\n                  autoComplete=\"email\"\r\n                  placeholder=\"emma@company.com\"\r\n                  className=\"mt-2\"\r\n                />\r\n              </div>\r\n              <div className=\"col-span-full sm:col-span-3\">\r\n                <Label\r\n                  htmlFor=\"birthyear\"\r\n                  className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                >\r\n                  Birth year\r\n                </Label>\r\n                <Input\r\n                  type=\"number\"\r\n                  id=\"birthyear\"\r\n                  name=\"year\"\r\n                  placeholder=\"1990\"\r\n                  className=\"mt-2\"\r\n                />\r\n              </div>\r\n              <div className=\"col-span-full sm:col-span-3\">\r\n                <Label\r\n                  htmlFor=\"role\"\r\n                  className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                >\r\n                  Role\r\n                </Label>\r\n                <Input\r\n                  type=\"text\"\r\n                  id=\"role\"\r\n                  name=\"role\"\r\n                  placeholder=\"Senior Manager\"\r\n                  disabled\r\n                  className=\"mt-2\"\r\n                />\r\n                <p className=\"mt-2 text-xs text-muted-foreground dark:text-muted-foreground\">\r\n                  Roles can only be changed by system admin.\r\n                </p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <Separator className=\"my-8\" />\r\n        <div className=\"grid grid-cols-1 gap-10 md:grid-cols-3\">\r\n          <div>\r\n            <h2 className=\"font-semibold text-foreground dark:text-foreground\">\r\n              Workspace settings\r\n            </h2>\r\n            <p className=\"mt-1 text-sm leading-6 text-muted-foreground dark:text-muted-foreground\">\r\n              Lorem ipsum dolor sit amet, consetetur sadipscing elitr.\r\n            </p>\r\n          </div>\r\n          <div className=\"sm:max-w-3xl md:col-span-2\">\r\n            <div className=\"grid grid-cols-1 gap-4 sm:grid-cols-6\">\r\n              <div className=\"col-span-full sm:col-span-3\">\r\n                <Label\r\n                  htmlFor=\"workspace-name\"\r\n                  className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                >\r\n                  Workspace name\r\n                </Label>\r\n                <Input\r\n                  type=\"text\"\r\n                  id=\"workspace-name\"\r\n                  name=\"workspace-name\"\r\n                  placeholder=\"Test workspace\"\r\n                  className=\"mt-2\"\r\n                />\r\n              </div>\r\n              <div className=\"col-span-full sm:col-span-3\">\r\n                <Label\r\n                  htmlFor=\"visibility\"\r\n                  className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                >\r\n                  Visibility\r\n                </Label>\r\n                <Select name=\"visibility\" defaultValue=\"private\">\r\n                  <SelectTrigger id=\"visibility\" className=\"mt-2\">\r\n                    <SelectValue placeholder=\"Select visibility\" />\r\n                  </SelectTrigger>\r\n                  <SelectContent>\r\n                    <SelectItem value=\"public\">Public</SelectItem>\r\n                    <SelectItem value=\"private\">Private</SelectItem>\r\n                  </SelectContent>\r\n                </Select>\r\n              </div>\r\n              <div className=\"col-span-full\">\r\n                <Label\r\n                  htmlFor=\"workspace-description\"\r\n                  className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                >\r\n                  Workspace description\r\n                </Label>\r\n                <Textarea\r\n                  id=\"workspace-description\"\r\n                  name=\"workspace-description\"\r\n                  className=\"mt-2\"\r\n                  rows={4}\r\n                />\r\n                <p className=\"mt-2 text-xs text-muted-foreground dark:text-muted-foreground\">\r\n                  Note: description provided will not be displayed externally.\r\n                </p>\r\n              </div>\r\n            </div>\r\n          </div>\r\n        </div>\r\n        <Separator className=\"my-8\" />\r\n        <div className=\"grid grid-cols-1 gap-10 md:grid-cols-3\">\r\n          <div>\r\n            <h2 className=\"font-semibold text-foreground dark:text-foreground\">\r\n              Notification settings\r\n            </h2>\r\n            <p className=\"mt-1 text-sm leading-6 text-muted-foreground dark:text-muted-foreground\">\r\n              Lorem ipsum dolor sit amet, consetetur sadipscing elitr.\r\n            </p>\r\n          </div>\r\n          <div className=\"sm:max-w-3xl md:col-span-2\">\r\n            <fieldset>\r\n              <legend className=\"text-sm font-medium text-foreground dark:text-foreground\">\r\n                Team\r\n              </legend>\r\n              <p className=\"mt-1 text-sm leading-6 text-muted-foreground dark:text-muted-foreground\">\r\n                Configure the types of team alerts you want to receive.\r\n              </p>\r\n              <div className=\"mt-2\">\r\n                <div className=\"flex items-center gap-x-3 py-1\">\r\n                  <Checkbox\r\n                    id=\"team-requests\"\r\n                    name=\"team-requests\"\r\n                    defaultChecked\r\n                  />\r\n                  <Label\r\n                    htmlFor=\"team-requests\"\r\n                    className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                  >\r\n                    Team join requests\r\n                  </Label>\r\n                </div>\r\n                <div className=\"flex items-center gap-x-3 py-1\">\r\n                  <Checkbox\r\n                    id=\"team-activity-digest\"\r\n                    name=\"team-activity-digest\"\r\n                  />\r\n                  <Label\r\n                    htmlFor=\"team-activity-digest\"\r\n                    className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                  >\r\n                    Weekly team activity digest\r\n                  </Label>\r\n                </div>\r\n              </div>\r\n            </fieldset>\r\n            <fieldset className=\"mt-6\">\r\n              <legend className=\"text-sm font-medium text-foreground dark:text-foreground\">\r\n                Usage\r\n              </legend>\r\n              <p className=\"mt-1 text-sm leading-6 text-muted-foreground dark:text-muted-foreground\">\r\n                Configure the types of usage alerts you want to receive.\r\n              </p>\r\n              <div className=\"mt-2\">\r\n                <div className=\"flex items-center gap-x-3 py-1\">\r\n                  <Checkbox id=\"api-requests\" name=\"api-requests\" />\r\n                  <Label\r\n                    htmlFor=\"api-requests\"\r\n                    className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                  >\r\n                    API requests\r\n                  </Label>\r\n                </div>\r\n                <div className=\"flex items-center gap-x-3 py-1\">\r\n                  <Checkbox\r\n                    id=\"workspace-execution\"\r\n                    name=\"workspace-execution\"\r\n                  />\r\n                  <Label\r\n                    htmlFor=\"workspace-execution\"\r\n                    className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                  >\r\n                    Workspace loading times\r\n                  </Label>\r\n                </div>\r\n                <div className=\"flex items-center gap-x-3 py-1\">\r\n                  <Checkbox\r\n                    id=\"query-caching\"\r\n                    name=\"query-caching\"\r\n                    defaultChecked\r\n                  />\r\n                  <Label\r\n                    htmlFor=\"query-caching\"\r\n                    className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                  >\r\n                    Query caching\r\n                  </Label>\r\n                </div>\r\n                <div className=\"flex items-center gap-x-3 py-1\">\r\n                  <Checkbox id=\"storage\" name=\"storage\" defaultChecked />\r\n                  <Label\r\n                    htmlFor=\"storage\"\r\n                    className=\"text-sm font-medium text-foreground dark:text-foreground\"\r\n                  >\r\n                    Storage\r\n                  </Label>\r\n                </div>\r\n              </div>\r\n            </fieldset>\r\n          </div>\r\n        </div>\r\n        <Separator className=\"my-8\" />\r\n        <div className=\"flex items-center justify-end space-x-4\">\r\n          <Button type=\"button\" variant=\"outline\" className=\"whitespace-nowrap\">\r\n            Go back\r\n          </Button>\r\n          <Button type=\"submit\" className=\"whitespace-nowrap\">\r\n            Save settings\r\n          </Button>\r\n        </div>\r\n      </form>\r\n    </div>\r\n  );\r\n}\r\n"
    }
