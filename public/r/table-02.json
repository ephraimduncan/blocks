      "target": "components/table-02.tsx",
      "content": "import { Badge } from \"@/components/ui/badge\"\nimport { Table, TableBody, TableCell, TableHead, TableHeader, TableRow } from \"@/components/ui/table\"\nimport { Clock, XCircle, CheckCircle2, ArrowRight } from \"lucide-react\"\n\nconst orders = [\n  {\n    id: \"ORD-1247\",\n    customer: \"Sarah Williams\",\n    status: \"completed\",\n    payment: \"Credit Card\",\n    total: \"$234.50\",\n    date: \"15 Mar 2024\",\n  },\n  {\n    id: \"ORD-1248\",\n    customer: \"Michael Chen\",\n    status: \"pending\",\n    payment: \"PayPal\",\n    total: \"$89.99\",\n    date: \"16 Mar 2024\",\n  },\n  {\n    id: \"ORD-1249\",\n    customer: \"Emma Rodriguez\",\n    status: \"cancelled\",\n    payment: \"Debit Card\",\n    total: \"$156.25\",\n    date: \"16 Mar 2024\",\n  },\n  {\n    id: \"ORD-1250\",\n    customer: \"James Wilson\",\n    status: \"completed\",\n    payment: \"Credit Card\",\n    total: \"$412.00\",\n    date: \"17 Mar 2024\",\n  },\n  {\n    id: \"ORD-1251\",\n    customer: \"Olivia Taylor\",\n    status: \"pending\",\n    payment: \"Bank Transfer\",\n    total: \"$78.50\",\n    date: \"18 Mar 2024\",\n  },\n  {\n    id: \"ORD-1252\",\n    customer: \"Lucas Anderson\",\n    status: \"completed\",\n    payment: \"Credit Card\",\n    total: \"$299.99\",\n    date: \"18 Mar 2024\",\n  },\n  {\n    id: \"ORD-1253\",\n    customer: \"Sophia Martinez\",\n    status: \"pending\",\n    payment: \"PayPal\",\n    total: \"$167.75\",\n    date: \"19 Mar 2024\",\n  },\n]\n\nfunction getStatusBadge(status: string) {\n  switch (status) {\n    case \"pending\":\n      return (\n        <Badge\n          variant=\"outline\"\n          className=\"bg-amber-500/15 text-amber-700 hover:bg-amber-500/25 dark:bg-amber-500/10 dark:text-amber-300 dark:hover:bg-amber-500/20 border-0\"\n        >\n          <Clock className=\"mr-1.5 size-3\" />\n          Pending\n        </Badge>\n      )\n    case \"cancelled\":\n      return (\n        <Badge\n          variant=\"outline\"\n          className=\"bg-rose-500/15 text-rose-700 hover:bg-rose-500/25 dark:bg-rose-500/10 dark:text-rose-400 dark:hover:bg-rose-500/20 border-0\"\n        >\n          <XCircle className=\"mr-1.5 size-3\" />\n          Cancelled\n        </Badge>\n      )\n    case \"completed\":\n      return (\n        <Badge\n          variant=\"outline\"\n          className=\"bg-green-500/15 text-green-700 hover:bg-green-500/25 dark:bg-green-500/10 dark:text-green-400 dark:hover:bg-green-500/20 border-0\"\n        >\n          <CheckCircle2 className=\"mr-1.5 size-3\" />\n          Completed\n        </Badge>\n      )\n    default:\n      return <Badge variant=\"secondary\">{status}</Badge>\n  }\n}\n\nexport default function Table02() {\n  return (\n    <div className=\"w-full max-w-7xl mx-auto p-8\">\n      <div className=\"mb-6 flex items-start justify-between\">\n        <div>\n          <h1 className=\"text-lg font-semibold leading-none tracking-tight\">Recent Orders</h1>\n          <p className=\"mt-1.5 max-w-lg text-pretty text-sm text-muted-foreground\">\n            View and manage customer orders from the past week. Last updated{\" \"}\n            <strong className=\"text-foreground\">19 Mar 2024</strong>.\n          </p>\n        </div>\n        <a\n          href=\"/orders/all\"\n          className=\"relative isolate inline-flex items-center justify-center gap-x-2 rounded-lg bg-secondary px-3.5 py-2 text-sm font-medium text-secondary-foreground shadow-sm ring-1 ring-inset ring-foreground/15 transition-colors hover:bg-secondary/85 focus:outline-none focus-visible:outline focus-visible:outline-2 focus-visible:outline-offset-2 focus-visible:outline-ring focus-visible:ring-2 focus-visible:ring-offset-3 focus-visible:ring-offset-background sm:px-3 sm:py-1.5 sm:text-sm\"\n        >\n          View All\n          <ArrowRight className=\"size-4\" />\n        </a>\n      </div>\n\n      <div className=\"w-full rounded-lg border border-zinc-400/70 bg-background/50 p-4 shadow-sm dark:border-zinc-700\">\n        <div className=\"overflow-x-auto\">\n          <Table>\n            <TableHeader>\n              <TableRow>\n                <TableHead>Order ID</TableHead>\n                <TableHead>Customer</TableHead>\n                <TableHead>Status</TableHead>\n                <TableHead>Payment</TableHead>\n                <TableHead>Total</TableHead>\n                <TableHead>Date</TableHead>\n              </TableRow>\n            </TableHeader>\n            <TableBody>\n              {orders.map((order) => (\n                <TableRow key={order.id} className=\"cursor-pointer\">\n                  <TableCell className=\"font-mono text-sm\">\n                    <Badge variant=\"secondary\" className=\"bg-secondary hover:bg-muted dark:bg-secondary dark:hover:bg-muted\">\n                      {order.id}\n                    </Badge>\n                  </TableCell>\n                  <TableCell className=\"font-medium\">{order.customer}</TableCell>\n                  <TableCell>{getStatusBadge(order.status)}</TableCell>\n                  <TableCell>\n                    <span className=\"text-sm text-muted-foreground\">{order.payment}</span>\n                  </TableCell>\n                  <TableCell className=\"font-semibold\">{order.total}</TableCell>\n                  <TableCell className=\"text-sm text-muted-foreground\">{order.date}</TableCell>\n                </TableRow>\n              ))}\n            </TableBody>\n          </Table>\n        </div>\n      </div>\n    </div>\n  )\n}\n"
    }
  ],
  "categories": [
    "tables"
  ]
}